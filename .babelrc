{
    "plugins": [
        "babel-plugin-module-alias",
        "react-require"
    ],
    "extra": {
        "module-alias": [{
            "src": "./src/client/scripts/",
            "expose": "client"
        }, {
            "src": "./src/server/",
            "expose": "server"
        }, {
            "src": "./src/common/",
            "expose": "common"
        }, {
            "src": "./test/",
            "expose": "test"
        }]
    }
}
